{
  "name": "posthtml-beautify",
  "version": "0.1.7",
  "description": "A posthtml plugin to beautify you html files",
  "license": "MIT",
  "repository": "gitscrum/posthtml-beautify",
  "homepage": "gitscrum/posthtml-beautify#readme",
  "bugs": "gitscrum/posthtml-beautify/issues",
  "author": {
    "name": "Ivan Demidov",
    "email": "Scrum@list.ru",
    "url": "https://twitter.com/Scrum_"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "commitmsg": "conventional-changelog-lint -e",
    "postpublish": "conventional-changelog -i changelog.md -s -r 0 && git commit -am \"chore: Update changelog\"",
    "patch": "np patch --any-branch --no-yarn",
    "minor": "np minor --any-branch --no-yarn",
    "major": "np major --any-branch --no-yarn",
    "prepublish": "npm run build",
    "docs": "webpack --progress --env.prod -p",
    "build": "rimraf lib && babel src -d lib --ignore docs",
    "lintjs": "eslint ./{src,test}/*.js",
    "lintmd": "eslint --ext md --rule indent: [error, 4] .",
    "linthtml": "eslint **/*.{html,htm}",
    "pretest": "clinton && npm run lintjs && npm run lintmd && npm run linthtml",
    "test": "nyc ava",
    "serve": "webpack-dashboard -- webpack-dev-server --content-base docs/ --open"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin",
    "beautify",
    "beautifier",
    "code-quality"
  ],
  "dependencies": {
    "deepmerge": "^1.3.2",
    "posthtml-parser": "0.2.1",
    "posthtml-render": "1.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^3.2.0",
    "@commitlint/config-angular": "^3.1.1",
    "ava": "*",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "clinton": "^0.13.0",
    "conventional-changelog-cli": "^1.3.3",
    "conventional-changelog-lint": "^2.1.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.2",
    "eslint": "^4.6.1",
    "eslint-config-xo": "^0.18.2",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-ava": "^4.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-markdown": "^1.0.0-beta.2",
    "eslint-plugin-xo": "^1.0.0",
    "highlight.js": "^9.11.0",
    "husky": "^0.14.3",
    "is-promise": "^2.1.0",
    "np": "^2.14.1",
    "nyc": "^11.2.1",
    "posthtml": "^0.9.2",
    "posthtml-include": "^1.1.0",
    "posthtml-modules": "^0.4.2",
    "rimraf": "^2.6.1",
    "vue": "^2.3.3",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "useBuiltIns": true
        }
      ],
      [
        "babili",
        {
          "mangle": false
        }
      ]
    ],
    "plugins": [
      "array-includes",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "html",
      "ava",
      "xo",
      "babel",
      "markdown"
    ],
    "extends": [
      "xo",
      "plugin:xo/recommended",
      "plugin:ava/recommended"
    ]
  },
  "clinton": {
    "ignores": [
      "test/**",
      "tmp/**",
      "lib/**",
      "*.{html,jpg}"
    ],
    "rules": {
      "pkg-main": [
        "off"
      ],
      "xo": [
        "off"
      ]
    }
  },
  "testen": {
    "node": [
      "4",
      "5",
      "6",
      "7"
    ]
  }
}
